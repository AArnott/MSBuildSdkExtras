<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup Condition="$(_ExtrasPortableTfmCount) == 2 and                             
                            ($(_ExtrasPortableTfms.Contains('net45')) and !$(_ExtrasPortableTfms.Contains('net451'))) and
                            (($(_ExtrasPortableTfms.Contains('win')) and !$(_ExtrasPortableTfms.Contains('win81'))) or $(_ExtrasPortableTfms.Contains('win8')) or $(_ExtrasPortableTfms.Contains('win80')))">
    <TargetFrameworkVersion>v4.5</TargetFrameworkVersion>
    <TargetFrameworkProfile>Profile7</TargetFrameworkProfile>
  </PropertyGroup>

  <PropertyGroup Condition="$(_ExtrasPortableTfmCount) == 2 and                             
                            ($(_ExtrasPortableTfms.Contains('net45')) and !$(_ExtrasPortableTfms.Contains('net451'))) and
                            ($(_ExtrasPortableTfms.Contains('wp8')) or $(_ExtrasPortableTfms.Contains('wp80')) and !$(_ExtrasPortableTfms.Contains('wp81')))">
    <TargetFrameworkVersion>v4.5</TargetFrameworkVersion>
    <TargetFrameworkProfile>Profile49</TargetFrameworkProfile>
  </PropertyGroup>

  <PropertyGroup Condition="$(_ExtrasPortableTfmCount) == 3 and                             
                            ($(_ExtrasPortableTfms.Contains('net45')) and !$(_ExtrasPortableTfms.Contains('net451'))) and
                            (($(_ExtrasPortableTfms.Contains('win')) and !$(_ExtrasPortableTfms.Contains('win81'))) or $(_ExtrasPortableTfms.Contains('win8')) or $(_ExtrasPortableTfms.Contains('win80'))) and
                            ($(_ExtrasPortableTfms.Contains('wp8')) or $(_ExtrasPortableTfms.Contains('wp80')) and !$(_ExtrasPortableTfms.Contains('wp81')))">
    <TargetFrameworkVersion>v4.5</TargetFrameworkVersion>
    <TargetFrameworkProfile>Profile78</TargetFrameworkProfile>
  </PropertyGroup>

  <PropertyGroup Condition="$(_ExtrasPortableTfmCount) == 3 and                             
                            ($(_ExtrasPortableTfms.Contains('net45')) and !$(_ExtrasPortableTfms.Contains('net451'))) and
                            (($(_ExtrasPortableTfms.Contains('win')) and !$(_ExtrasPortableTfms.Contains('win81'))) or $(_ExtrasPortableTfms.Contains('win8')) or $(_ExtrasPortableTfms.Contains('win80'))) and
                            ($(_ExtrasPortableTfms.Contains('wpa81')) or $(_ExtrasPortableTfms.Contains('wpa')))">
    <TargetFrameworkVersion>v4.5</TargetFrameworkVersion>
    <TargetFrameworkProfile>Profile111</TargetFrameworkProfile>
  </PropertyGroup>

  <PropertyGroup Condition="$(_ExtrasPortableTfmCount) == 4 and                             
                            ($(_ExtrasPortableTfms.Contains('net45')) and !$(_ExtrasPortableTfms.Contains('net451'))) and
                            (($(_ExtrasPortableTfms.Contains('win')) and !$(_ExtrasPortableTfms.Contains('win81'))) or $(_ExtrasPortableTfms.Contains('win8')) or $(_ExtrasPortableTfms.Contains('win80'))) and
                            ($(_ExtrasPortableTfms.Contains('wpa')) or $(_ExtrasPortableTfms.Contains('wpa81'))) and
                            ($(_ExtrasPortableTfms.Contains('wp8')) or $(_ExtrasPortableTfms.Contains('wp80')) and !$(_ExtrasPortableTfms.Contains('wp81')))">
    <TargetFrameworkVersion>v4.5</TargetFrameworkVersion>
    <TargetFrameworkProfile>Profile259</TargetFrameworkProfile>
  </PropertyGroup>
  
</Project>